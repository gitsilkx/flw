}    
.header-serch-container input[type="text"] {
  float: left;
  height: 26px;
  margin-left: 10px;
  margin-top: 0;
  padding: 0 6px;
  width: 30%;
}    
.h_oderSec {
  left: 50%;
  margin-bottom: 20px;
  margin-left: -73px;
  margin-top: 40px;
  position: relative;
  top: 20px;
  width: 280px;
}
a.h_logo {
  display: inline-block;
  float: none;
  margin: 0 auto;
  width: 350px;
}    
a.h_logo span.h_logo_name {
  font-size: 27px;
}    
table.FeaturedProducts tr.pagination_area td {
  height: inherit;
  width: 100%;
}    

.search_sec {
  float: left;
  margin: -4px auto;
