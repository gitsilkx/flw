width: 230px !important;
}
.f-navigation ul li.f-OccasionArea {
  float: left;
width: 220px !important;
}
.f-navigation ul li.f-FlowerTypeArea {
  float: left;
  width: 230px !important;
}
.f-navigation ul li.f-ExpressYourselfArea {
  float: left;
  width: 230px !important;
}    
.logo_area_footer {
  margin-left: 230px;
  margin-top: -240px;
}    
ul.listing {
